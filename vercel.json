{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/search_complexes.js": {
      "maxDuration": 30
    },
    "api/fetch_listings.js": {
      "maxDuration": 60
    },
    "api/download_excel.js": {
      "maxDuration": 60
    }
  }
}